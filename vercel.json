{
  "version": 2,
  "builds": [
    { "src": "api/app.py", "use": "@vercel/python" },
    { "src": "*.html", "use": "@vercel/static" },
    { "src": "*.css", "use": "@vercel/static" },
    { "src": "*.js", "use": "@vercel/static" }
  ],
  "rewrites": [
    { "source": "/", "destination": "/wordle.html" }
  ]
}
